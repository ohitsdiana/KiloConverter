   <children>
      <Label fx:id="promptLabel" layoutX="28.0" layoutY="54.0" text="Enter a distance in Kilometers:">
         <font>
            <Font name="Cambria" size="24.0" />
         </font></Label>
      <TextField fx:id="kilometerTextField" layoutX="373.0" layoutY="58.0" />
      <Button fx:id="convertbutton" layoutX="185.0" layoutY="244.0" mnemonicParsing="false" onAction="#convertButtonListener" prefHeight="40.0" prefWidth="218.0" text="Convert to Miles" />
      <Label fx:id="outputLabel" layoutX="63.0" layoutY="111.0" prefHeight="38.0" prefWidth="431.0">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
